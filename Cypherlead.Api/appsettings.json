{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] ({Application}/{MachineName}/{ThreadId}) {Message}{NewLine}{Exception}"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/cypler_lead_logs.log",
          "rollingInterval": "Hour"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
